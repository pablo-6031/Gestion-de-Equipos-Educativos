             xmlns:local="clr-namespace:Gestion_de_Equipos_Educativos"
             StartupUri="Login.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MiDiccionario.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Estilos/BotonEstilos.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Estilos/Colores.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
